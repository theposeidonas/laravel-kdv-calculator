{
    "name": "theposeidonas/laravel-kdv-calculator",
    "description": "KDV hesaplamak için oluşturulmuş Laravel Paketi",
    "keywords": ["laravel", "KDV", "VAT", "TAX"],
    "license": "MIT",
    "version": "v1.0.1",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Theposeidonas\\LaravelKdvCalculator\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Baran Arda",
            "email": "info@baranarda.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Theposeidonas\\LaravelKdvCalculator\\KdvCalculatorServiceProvider"
            ],
            "aliases": {
                "KDV": "Theposeidonas\\LaravelKdvCalculator\\Facades\\KDV"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "require": {
        "php": "^8.1.0",
        "laravel/framework": "^9.0|^10|^11"
    },
    "require-dev": {
        "orchestra/workbench": "^7.3|^8.5|9.x-dev",
        "phpunit/phpunit": "^9.0|^10"
    }
}
